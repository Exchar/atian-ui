{
  "name": "atian-ui",
  "version": "0.1.2",
  "description": "A React component library for building user interfaces.",
  "author": "Exchar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/Exchar/atian-ui.git"
  },
  "homepage": "https://gitee.com/Exchar/atian-ui",
  "bugs": {
    "url": "https://gitee.com/Exchar/atian-ui/issues"
  },
  "keywords": [
    "react",
    "component",
    "library",
    "ui",
    "design"
  ],
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/umd/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/umd/index.js"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "rimraf dist &&vite build",
    "build:css": "sass src/style/index.scss dist/style.css",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "dev": "storybook dev -p 6006",
    "build-storybook": "storybook build --docs"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "animate.css": "^4.1.1",
    "classnames": "^2.5.1",
    "react-transition-group": "^4.4.5",
    "sass": "^1.86.0",
    "vite-plugin-graphql-loader": "^4.0.4"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3",
    "@eslint/js": "^9.21.0",
    "@storybook/addon-actions": "8.6.11",
    "@storybook/addon-docs": "^8.6.11",
    "@storybook/addon-essentials": "8.6.11",
    "@storybook/addon-onboarding": "8.6.11",
    "@storybook/addon-viewport": "^8.6.11",
    "@storybook/blocks": "8.6.11",
    "@storybook/builder-vite": "^8.6.11",
    "@storybook/experimental-addon-test": "8.6.11",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "8.6.11",
    "@storybook/react-vite": "^8.6.11",
    "@storybook/test": "8.6.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/classnames": "^2.3.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/react-transition-group": "^4.4.12",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/browser": "3.0.9",
    "@vitest/coverage-v8": "3.0.9",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^15.15.0",
    "jsdom": "^26.0.0",
    "playwright": "^1.51.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "remark-gfm": "^4.0.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.24",
    "storybook": "8.6.11",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.9"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
